{
  "name": "HubRise",
  "description": "HubRise website",
  "version": "0.1.0",
  "scripts": {
    "build": "yarn clean && gatsby build",
    "clean": "gatsby clean",
    "start:dev": "yarn clean && gatsby develop",
    "start:prod": "yarn build && gatsby serve --port 9009",
    "cy:open": "cypress open",
    "cy:run": "cross-env CYPRESS_BASE_URL=http://localhost:9009 cypress run",
    "test:unit-testing": "jest --config='./jest/config.js'",
    "test:e2e": "start-server-and-test start:dev http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test start:prod http://localhost:9009 cy:run",
    "test:all:ci": "yarn test:unit-testing && yarn test:e2e:ci"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@mdx-js/mdx": "^1.6.16",
    "@mdx-js/react": "^1.6.16",
    "classnames": "^2.2.6",
    "font-awesome": "^4.7.0",
    "formik": "^2.1.5",
    "foundation-sites": "^6.6.3",
    "gatsby": "2.24.15",
    "gatsby-image": "^2.4.14",
    "gatsby-plugin-client-side-redirect": "^1.1.0",
    "gatsby-plugin-google-analytics": "^2.3.13",
    "gatsby-plugin-manifest": "^2.4.21",
    "gatsby-plugin-mdx": "^1.2.30",
    "gatsby-plugin-offline": "^3.2.21",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sass": "^2.3.12",
    "gatsby-plugin-sharp": "^2.6.23",
    "gatsby-remark-copy-linked-files": "^2.3.12",
    "gatsby-remark-images": "^3.3.22",
    "gatsby-source-filesystem": "^2.3.23",
    "gatsby-transformer-sharp": "^2.5.11",
    "i18next": "^19.6.3",
    "i18next-browser-languagedetector": "^5.0.0",
    "jquery": "^3.5.1",
    "motion-ui": "^2.0.3",
    "node-sass": "^4.14.1",
    "prism-react-renderer": "^1.1.1",
    "prismjs": "^1.20.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.7.0",
    "react-slick": "^0.27.1",
    "react-use": "^15.3.3",
    "remark": "^12.0.1",
    "remark-html": "^12.0.0",
    "remark-preset-lint-recommended": "^4.0.1",
    "slick-carousel": "^1.8.1",
    "webpack": "^4.44.0",
    "what-input": "^5.2.10",
    "yup": "^0.29.2"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^10.4.7",
    "@types/js-yaml": "^3.12.5",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-plugin-prismjs": "^2.0.1",
    "babel-preset-gatsby": "^0.5.4",
    "cross-env": "^7.0.2",
    "cypress": "^4.11.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "gatsby-plugin-sentry": "^1.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "js-yaml": "^3.14.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.11.2",
    "typescript": "^3.9.7"
  },
  "lint-staged": {
    "**/*.{json,scss,md,mdx}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "gatsby",
    "hubrise"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:HubRise/website.git"
  },
  "bugs": {
    "url": "https://github.com/HubRise/website/issues"
  }
