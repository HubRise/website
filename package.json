{
  "name": "HubRise",
  "description": "HubRise website",
  "version": "0.1.0",
  "scripts": {
    "build": "yarn clean && yarn gatsby build",
    "clean": "yarn gatsby clean",
    "eslint": "eslint src content --fix --ext ts,tsx,md,mdx",
    "gatsby": "node ./node_modules/.bin/gatsby",
    "start:dev": "yarn gatsby develop",
    "start:prod": "yarn gatsby serve --port 9009",
    "test": "jest --config config/jest.config.js",
    "cypress:open": "start-server-and-test start:dev http://localhost:8000 'yarn cypress open'",
    "cypress:run": "start-server-and-test start:prod http://localhost:9009 'yarn cross-env CYPRESS_BASE_URL=http://localhost:9009 cypress run'",
    "test:all": "yarn test && yarn cypress:run"
  },
  "dependencies": {
