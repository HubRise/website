{
  "name": "HubRise",
  "description": "HubRise website",
  "version": "0.1.0",
  "scripts": {
    "build": "yarn clean && gatsby build",
    "clean": "gatsby clean",
    "gatsby": "node ./node_modules/.bin/gatsby",
    "start:dev": "gatsby develop",
    "start:prod": "yarn build && gatsby serve --port 9009",
    "cy:open": "cypress open",
    "cy:run": "cross-env CYPRESS_BASE_URL=http://localhost:9009 cypress run",
    "test:unit-testing": "jest --config='./jest/config.js'",
    "test:e2e": "start-server-and-test start:dev http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test start:prod http://localhost:9009 cy:run",
    "test:all:ci": "yarn test:unit-testing && yarn test:e2e:ci"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@sentry/react": "^6.5.0",
    "classnames": "^2.3.1",
    "font-awesome": "^4.7.0",
    "formik": "^2.2.8",
    "foundation-sites": "^6.6.3",
    "gatsby": "^3.6.2",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-client-side-redirect": "^1.1.0",
    "gatsby-plugin-google-analytics": "^3.6.0",
    "gatsby-plugin-image": "^1.6.0",
    "gatsby-plugin-manifest": "^3.6.0",
    "gatsby-plugin-mdx": "^2.6.0",
    "gatsby-plugin-offline": "^4.6.0",
    "gatsby-plugin-react-helmet": "^4.6.0",
    "gatsby-plugin-sass": "^4.6.0",
    "gatsby-plugin-sharp": "^3.6.0",
    "gatsby-remark-copy-linked-files": "^4.3.0",
    "gatsby-remark-images": "^5.3.0",
    "gatsby-source-filesystem": "^3.6.0",
    "gatsby-transformer-sharp": "^3.6.0",
    "i18next": "^20.3.1",
    "i18next-browser-languagedetector": "^6.1.1",
    "jquery": "^3.6.0",
    "motion-ui": "^2.0.3",
    "node-sass": "^6.0.0",
    "prism-react-renderer": "^1.2.1",
    "prismjs": "^1.23.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.10.0",
    "react-slick": "^0.28.1",
    "react-use": "^17.2.4",
    "remark": "^13.0.0",
    "remark-html": "^13.0.1",
    "remark-preset-lint-recommended": "^5.0.0",
    "slick-carousel": "^1.8.1",
    "webpack": "^5.38.1",
    "what-input": "^5.2.10",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@types/js-yaml": "^4.0.1",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.2",
    "babel-plugin-prismjs": "^2.0.1",
    "babel-preset-gatsby": "^1.6.0",
    "cross-env": "^7.0.3",
    "cypress": "^7.4.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby-plugin-sentry": "^1.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.3",
    "js-yaml": "^4.1.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "prop-types": "^15.7.2",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.12.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "lint-staged": {
    "**/*.{json,scss,md,mdx}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "gatsby",
    "hubrise"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:HubRise/website.git"
  },
  "bugs": {
    "url": "https://github.com/HubRise/website/issues"
  }
}
